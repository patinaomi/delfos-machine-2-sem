@model IEnumerable<LexusTech.Models.Clinica>

<section class="p-5">

    <h2>Lista de Usuários</h2>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nome</th>
                <th>Endereco</th>
                <th>Telefone</th>
                <th>Avaliação</th>
                <th>Preço Médio</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var clinica in Model)
            {
                <tr>
                    <td>@clinica.Id</td>
                    <td>@clinica.Nome</td>
                    <td>@clinica.Endereco</td>
                    <td>@clinica.Telefone</td>
                    <td>@clinica.Avaliacao</td>
                    <td>@clinica.PrecoMedio</td>
                </tr>
            }
        </tbody>
    </table>

    <div class="form-group pt-3">
        <a asp-controller="Clinica" asp-action="Criar" class="btn btn-warning">Voltar</a>
    </div>

</section>